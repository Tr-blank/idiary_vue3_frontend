{
  "name": "idiary_vue3_frontend",
  "homepage": "https://tr-blank.github.io/idiary_vue3_frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start:dev": "set NODE_ENV=dev && vite",
    "start:prod": "set NODE_ENV=prod && vite",
    "start": "node server/app.js",
    "build": "set NODE_ENV=prod && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "predeploy": "set NODE_ENV=prod && npm run build",
    "deploy": "set NODE_ENV=prod && vite build && gh-pages -d dist"
  },
  "dependencies": {
    "@heroicons/vue": "^2.1.3",
    "axios": "^1.6.8",
    "element-plus": "^2.7.5",
    "gh-pages": "^6.1.1",
    "pinia": "^2.1.7",
    "sass": "^1.75.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "postcss": "^8.4.38",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.3",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.1.6"
  }
}
